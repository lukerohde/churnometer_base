<h1>Summary</h1>

<% if @data && @data[0] %>
  <table>
    <tr>
      <% @data[0].each do |k, v| %>
        <th><%= k %></th>
      <% end %>
    </tr>
    <% @data.each do |row| %>
      <tr>
        <% row.each do |k, v| %>
          <td><%= v %></td>
        <% end %>
      </tr>
    <% end %>
  </table>
<% else %>
  <p>No data</p>
<% end %>

